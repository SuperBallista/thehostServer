{
  "name": "nest-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "get:front": "npm --prefix Frontend run build && rimraf front && copyfiles -u 2 \"Frontend/front/**/*\" front",
    "build": "nest build --webpack && npm run copy:sql && npm run get:front",
    "copy:sql": "copyfiles -u 1 src/**/*.sql dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npm run build && node dist/main",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "pm2:start": "npm run build && pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop all",
    "pm2:restart": "npm run build && pm2 reload ecosystem.config.js --env production",
    "pm2:logs": "pm2 logs",
    "pm2:status": "pm2 status",
    "docker:up": "docker compose up -d && docker compose -f docker-compose-mysql.yml up -d && docker compose -f docker-compose-caddy.yml up -d",
    "docker:down": "docker compose down && docker compose -f docker-compose-mysql.yml down && docker compose -f docker-compose-caddy.yml down",
    "docker:mysql": "docker compose -f docker-compose-mysql.yml up -d",
    "docker:redis": "docker compose up -d",
    "docker:caddy": "docker compose -f docker-compose-caddy.yml up -d",
    "docker:logs": "docker compose logs -f",
    "docker:ps": "docker ps",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.1",
    "@google/generative-ai": "^0.21.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-socket.io": "^10.2.8",
    "@nestjs/websockets": "^10.2.8",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "mysql2": "^3.6.5",
    "node-fetch": "^3.3.2",
    "ollama": "^0.5.9",
    "openai": "^4.73.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "redis": "^4.6.11",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-jwt": "^3.0.8",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "globals": "^16.0.0",
    "jest": "^29.5.0",
    "pm2": "^6.0.8",
    "prettier": "^3.0.0",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.1",
    "typescript": "^5.1.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
